<?xml version="1.0" encoding="utf-8" ?>
<SourceAnalyzer Name="StyleCop+">
	<Description>
		Plug-in that extends original StyleCop features. Some settings for this plug-in are taken out to the separate StyleCop+ settings page.
	</Description>
	<Properties>
		<StringProperty Name="NamespaceNamingRule" DefaultValue="$(AaBb)" FriendlyName="Namespace" Description="Naming rules for namespaces." />
		<StringProperty Name="InterfaceNamingRule" DefaultValue="I$(AaBb)" FriendlyName="Interface" Description="Naming rules for interfaces." />
		<StringProperty Name="ClassNamingRule" DefaultValue="$(AaBb)" FriendlyName="Class" Description="Naming rules for classes." />
		<StringProperty Name="StructNamingRule" DefaultValue="" FriendlyName="Struct" Description="Naming rules for structs." />
	</Properties>
</SourceAnalyzer>
